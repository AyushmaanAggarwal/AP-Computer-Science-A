import static java.lang.System.*;
import java.util.Scanner;

public class CharacterRunner
{
	public static void main ( String[] args )
	{
		Scanner keyboard = new Scanner(System.in);
		
		out.print("Enter a letter :: ");
		char letter = keyboard.next().charAt(0);
		CharacterAnalyzer test = new CharacterAnalyzer(letter);
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('1');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('a');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('7');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('D');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('2');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('X');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('Z');
		out.println(test);   //A
		CharacterAnalyzer test = new CharacterAnalyzer('9');
		out.println(test);   //A
		//add more test cases	

	}
}